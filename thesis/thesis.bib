	year = {2010},
	keywords = {accelerometer, accelerometers, Apple AppStore, Apple computers, App Phones, camera, Cameras, communication device, compasses, context, digital compass, embedded sensors, Environmental Monitoring, Global Positioning System, gps, gyroscope, gyroscopes, health care, healthcare, Intelligent sensors, microphone, microphones, Mobile communication, Mobile handsets, mobile phone sensing, sensors, smart phones, social networking (online), social networks, transportation},
	pages = {140--150},
