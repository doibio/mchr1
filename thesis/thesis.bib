	keywords = {Computational Biology, Computer graphics, Databases, Factual, data visualisation, Heparin, human computer interaction, Humans, interactive visualization technique, Medical Records Systems, Computerized, Pattern Recognition, Automated, temporal categorical searching, temporal ordering, temporal summaries, Thrombocytopenia, Time Factors},
	pages = {1049 --1056}
}

@inproceedings{rosner_spyn:_2008,
	address = {New York, NY, USA},
