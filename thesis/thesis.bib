	title = {{SmallWorlds}: {Visualizing} {Social} {Recommendations}},
	volume = {29},
	copyright = {© 2010 The Author(s) Journal compilation © 2010 The Eurographics Association and Blackwell Publishing Ltd.},
	issn = {1467-8659},
