	urldate = {2012-12-27},
	booktitle = {Proceedings of the {SIGCHI} {Conference} on {Human} {Factors} in {Computing} {Systems}},
	publisher = {ACM},
	author = {Lee, Matthew L. and Dey, Anind K.},
	year = {2011},
	keywords = {aging-in-place, awareness, embedded assessment, functional ability, older adults, reflection},
	pages = {2095--2104},
	annote = {context use: memory, routine, calendar
calendars (electronic or paper) can be a good source of memory cues.
 
