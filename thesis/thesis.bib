@inproceedings{saraiya_evaluation_2004,
	title = {An evaluation of microarray visualization tools for biological insight},
	abstract = {High-throughput experiments such as gene expression microarrays in the life sciences result in very large datasets. In response, a wide variety of visualization tools have been created to facilitate data analysis. Life scientists face a dilemma in choosing the best tool for their situation. The tool that works best for one scientist may not work well for another due to differences in the type of insight they are seeking from their data. A primary purpose of these visualization tools is to provide biologically-relevant insight into the data. Ideally, the user wants maximum information in the least possible time. In this paper we identify several characteristics of insight that enable us to recognize and quantify it. Based on this, we empirically evaluate five popular microarray visualization tools. Our conclusions guide life scientists in selecting the best tool for their data, and guide computer scientists in developing and evaluating better visualizations.},
	author = {Saraiya, Purvi and North, Chris and Duca, Karen},
	year = {2004},
	pages = {1--8}
}

