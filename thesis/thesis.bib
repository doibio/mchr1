@article{hartel_time_2010,
	title = {Time as a {Framework} for {Information} {Science}: {Insights} from the {Hobby} of {Gourmet} {Cooking}},
	volume = {15},
	shorttitle = {Time as a {Framework} for {Information} {Science}},
	url = {http://works.bepress.com/jenna_hartel/5},
	abstract = {Introduction. This paper proposes a framework for information science based upon time. The conception is an extension of insights gleaned from an exploratory, ethnographic study of information in the hobby of gourmet cooking. Method. I was a participant observer of the social world of the hobby of gourmet cooking and conducted 20 semi-structured interviews with gourmet hobby cooks, followed by a photographic inventory of their home-based culinary information collections. Analysis. Data was analyzed using grounded theory facilitated by NVivo software. While attempting to code diverse information activities and resources, time emerged as a critical aspect of the hobby and its information phenomena. Results. The hobby of gourmet cooking unfolds as three temporal arcs. There is the long-running career arc that lasts for years or decades and represents the cook's lifetime experience of the hobby. There is the subject arc, made up of shorter periods in which a topic organizes culinary activity. Then, there is the episode arc that entails distinct hands-on cooking projects that happen in real-time. Each arc serves as a context for certain information activities and harbors quintessential information resources. Conclusions. The temporal arcs are a springboard to entertain a time-sensitive framework of the discipline. Several research specialties of information science appear to have an affinity for one arc, where their concerns and problems come into sharpest view. Overall, information science research may seem unsynchronized on account of differing temporal perspectives.},
	number = {4},
	urldate = {2013-06-21},
	journal = {Information Research},
	author = {Hartel, Jenna K.},
	year = {2010},
	keywords = {ethnography, gourmet cooking, Information behaviour, serious leisure}
}

@article{carrie_armel_is_2013,
	series = {Special {Section}: {Transition} {Pathways} to a {Low} {Carbon} {Economy}},
	title = {Is disaggregation the holy grail of energy efficiency? {The} case of electricity},
	volume = {52},
	issn = {0301-4215},
	shorttitle = {Is disaggregation the holy grail of energy efficiency?},
	url = {http://www.sciencedirect.com/science/article/pii/S0301421512007446},
	doi = {10.1016/j.enpol.2012.08.062},
	abstract = {This paper aims to address two timely energy problems. First, significant low-cost energy reductions can be made in the residential and commercial sectors, but these savings have not been achievable to date. Second, billions of dollars are being spent to install smart meters, yet the energy saving and financial benefits of this infrastructure – without careful consideration of the human element – will not reach its full potential. We believe that we can address these problems by strategically marrying them, using disaggregation. Disaggregation refers to a set of statistical approaches for extracting end-use and/or appliance level data from an aggregate, or whole-building, energy signal. In this paper, we explain how appliance level data affords numerous benefits, and why using the algorithms in conjunction with smart meters is the most cost-effective and scalable solution for getting this data. We review disaggregation algorithms and their requirements, and evaluate the extent to which smart meters can meet those requirements. Research, technology, and policy recommendations are also outlined.},
	urldate = {2014-10-23},
	journal = {Energy Policy},
	author = {Carrie Armel, K. and Gupta, Abhay and Shrimali, Gireesh and Albert, Adrian},
	month = jan,
	year = {2013},
	keywords = {disaggregation, energy efficiency, smart meter},
	pages = {213--234},
	file = {ScienceDirect Full Text PDF:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/UJCFSD8I/Carrie Armel et al. - 2013 - Is disaggregation the holy grail of energy efficie.pdf:application/pdf;ScienceDirect Snapshot:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/A3CMVA3I/S0301421512007446.html:text/html}
