	month = oct,
	year = {2006},
	keywords = {AI learning, causal reasoning, cognitive systems, confirmation bias, Data-Frame model, feedback, intelligent systems applications, learning (artificial intelligence), macrocognitive model, sensemaking theory},
	pages = {88 --92},
	file = {IEEE Xplore Abstract Record:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/58CVWTPC/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/82NF5DA7/Klein et al. - 2006 - Making Sense of Sensemaking 2 A Macrocognitive Mo.pdf:application/pdf}
