	author = {Maitland, Julie and Chalmers, Matthew},
	year = {2010},
	keywords = {cardiac rehabilitation, dietary intake, health-related behavioural change, Physical activity, self-monitoring},
	pages = {1213--1222}
}

@incollection{keim_visual_2008-1,
