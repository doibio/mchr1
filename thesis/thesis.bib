	keywords = {Construction, evaluation, interaction, Planning, Scheduling, Visualization},
	pages = {68--82},
	file = {ScienceDirect Snapshot:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/ZE8VUPV9/S0926580512001458.html:text/html}
}

