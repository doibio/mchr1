	pages = {2659 --2668},
	file = {IEEE Xplore Abstract Record:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/DQ5THJTD/articleDetails.html:text/html;IEEE Xplore Full Text PDF:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/93PTP6JT/Wongsuphasawat and Gotz - 2012 - Exploring Flow, Factors, and Outcomes of Temporal .pdf:application/pdf}
