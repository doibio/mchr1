	keywords = {complex time series data, concept, Concept map, conceptual information, conceptual recurrence plot technique, conversation analysis, conversation transcripts, data stream, data visualisation, Data visualization, global-local pattern visualization, human discourse, Humans, Image color analysis, information visualization technique, interaction pattern discovery, language modelling, Pain, pattern revealing, plotting, point similarity measurement, recurrence, semantics, Surgery, term-based similarity, text analysis, text analysis., text utterances, time series, Time series analysis},
	pages = {988 --997}
}

