@article{keeley_model_1983,
	title = {Model {House} {Designs} for {Privacy}, {Security}, and {Social} {Interaction}},
	volume = {119},
	issn = {0022-4545},
	url = {http://www.tandfonline.com/doi/abs/10.1080/00224545.1983.9922824},
	doi = {10.1080/00224545.1983.9922824},
	abstract = {Summary College undergraduates were asked to construct models of houses that would promote privacy, security, or social interaction for the occupants. The physical features of the models were then analyzed and compared. Among other differences, privacy models had significantly more rooms, somewhat more corridors, and more exterior wall surfaces. Security houses were significantly smaller, and more often had double-insulated bedrooms. Social interaction houses had greater visibility among rooms, and fewer exterior wall surfaces. Differences also appeared between male, female, and mixed-pair designers in this study: among other features, models designed by males were more often bilaterally symmetrical, and models by females were rated more original. When males and females worked together on the models, the final products contained somewhat more male-preferred features, but each sex contributed certain features to the final form.},
