	keywords = {abstract visualizations, ambient visualization, art, artistic visualization, building management systems, data visualisation, Decision Making, decision making support, distributed point-of-consumption feedback device, energy conservation, environmental conservation efforts, feedback, informative art, pervasive computing technology, power consumption, power engineering computing, residential energy consumption, residential energy use feedback visualization, resource consumption, ubiquitous computing},
	pages = {2489 --2497}
}

@inproceedings{kwon_visual_2011,
