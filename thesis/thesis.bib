	author = {Li, Ian and Dey, Anind K. and Forlizzi, Jodi},
	year = {2011},
	keywords = {design features, discovery, personal informatics, phases, reflection, visualizations},
	pages = {405--414}
}

@inproceedings{schwanda_side_2011-1,
