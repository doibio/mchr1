	author = {Wäljas, Minna and Segerst{\textbackslash}aahl, Katarina and Väänänen-Vainio-Mattila, Kaisa and Oinas-Kukkonen, Harri},
	year = {2010},
	keywords = {conceptual framework, crossmedial interactions, cross-platform web services, field study, user experience (UX)},
	pages = {219--228}
}

