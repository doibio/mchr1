	journal = {SIGKDD Explor. Newsl.},
	author = {Kwapisz, Jennifer R. and Weiss, Gary M. and Moore, Samuel A.},
	month = mar,
	year = {2011},
	keywords = {accelerometer, activity recognition, cell phone, induction, sensor mining, sensors},
	pages = {74--82}
}

