	booktitle = {Pervasive {Computing}},
	publisher = {Springer Berlin Heidelberg},
	author = {Froehlich, Jon and Larson, Eric and Saba, Elliot and Campbell, Tim and Atlas, Les and Fogarty, James and Patel, Shwetak},
