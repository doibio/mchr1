	journal = {Computers in Human Behavior},
	author = {Wagner, Nicole and Hassanein, Khaled and Head, Milena},
	month = sep,
	year = {2010},
	keywords = {Aging, Computer use, literature review, older adults, Social Cognitive Theory},
	pages = {870--882},
	file = {ScienceDirect Full Text PDF:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/2DFA4B6G/Wagner et al. - 2010 - Computer use by older adults A multi-disciplinary.pdf:application/pdf;ScienceDirect Snapshot:/Users/dandanhuang/Library/Application Support/Zotero/Profiles/51xyjwmu.default/zotero/storage/A55ZH5WP/S0747563210000695.html:text/html}
