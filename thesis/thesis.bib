	publisher = {ACM},
	author = {Haque, Md and Kawsar, Ferdaus and Adibuzzaman, Mohammad and Ahamed, Sheikh and Love, Richard and Dowla, Rumana and Roe, David and Hossain, Syed and Selim, Reza},
