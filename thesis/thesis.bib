	booktitle = {Proceedings of the {International} {Working} {Conference} on {Advanced} {Visual} {Interfaces}},
	publisher = {ACM},
	author = {Barata, Gabriel and Nicolau, Hugo and Gon{\textbackslash}ccalves, Daniel},
	year = {2012},
	keywords = {computer usage, desktop activity, information visualization},
	pages = {465--472}
}

