booktitle = {-},
copyright = {Copyright 1994, IEE},
title = {Automatic background music generation based on actors' mood and motions},
journal = {Journal of Visualization and Computer Animation},
journal = {J. Vis. Comput. Animat. (UK)},
author = {Nakamura, J.-I. and Kaku, T. and Hyun, K. and Noma, T. and Yoshida, S.},
volume = { 5},
year = {1994/10/},
pages = {247 - 64},
issn = {1049-8907},
address = {Seoul, South Korea},
abstract = {Since adding background music and sound effects even to short animations is not simple, an automatic music generation system would help improve the total quality of computer generated animations. The paper describes a prototype system which automatically generates background music and sound effects for existing animations. The inputs to the system are music parameters (mood types and musical motifs) and motion parameters for individual scenes of an animation. Music is generated for each scene. The key for a scene is determined by considering the mood type and its degree, and the key of the previous scene. The melody for a scene is generated from the given motifs and the chord progression for the scene which is determined according to appropriate rules. The harmony accompaniment for a scene is selected based on the mood type. The rhythm accompaniment for a scene is selected based on the mood type and tempo. The sound effects for motions are determined according to the characteristics and intensity of the motions. Both the background music and sound effects are generated so that the transitions between scenes are smooth},
keywords = {computer animation;electronic music;human factors;knowledge based systems;music;},
} 


@inproceedings{rui07,
