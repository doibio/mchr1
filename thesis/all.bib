title = {Automated music video generation using web image resource},
journal = {ICASSP, IEEE Int. Conf. on Acoustics, Speech and Signal Processing - Proc.},
author = {Cai, Rui and Zhang, Lei and Jing, Feng and Lai, Wei and Ma, Wei-Ying},
volume = { 2},
year = {2007},
pages = {737-740 - },
issn = {1520-6149},
address = {Honolulu, HI, United States},
abstract = {In this paper, we proposed a novel prototype of automated music video generation using web image resource. In this prototype, the salient words/phrases of a song's lyrics are first automatically extracted and then used as queries to retrieve related high-quality images from web search engines. To guarantee the coherence among the chosen images' visual representation and the music song, the returned images are further re-ranked and filtered based on their content characteristics such as color, face, landscape, as well as the song's mood type. Finally, those selected images are concatenated to generate a music video using the Photo2Video technique, based on the rhythm information of the music. Preliminary evaluations of the proposed prototype have shown promising results. &copy; 2007 IEEE.},
key = {Video streaming},
keywords = {Computer music;Image processing;Query processing;Software prototyping;Web services;},
} 


